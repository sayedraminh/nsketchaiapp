{
  "expo": {
    "name": "NSketch",
    "slug": "nsketchapp",
    "scheme": "nsketchapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nsketchai.app"
    },
    "android": {
      "edgeToEdgeEnabled": true
    },
    "plugins": [
      "expo-asset",
      "expo-build-properties",
      "expo-font",
      "expo-image-picker"
    ]
  }
}
